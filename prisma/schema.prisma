// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model AppError {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  message     String
  occurredAt  String
  triggeredBy String

  @@map("app_errors")
}
