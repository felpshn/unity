{
  "name": "saturn",
  "version": "0.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node -r ts-node/register -r tsconfig-paths/register dist/index.js",
    "dev": "tsnd --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc --project tsconfig.json&&tscpaths -p tsconfig.json -s src -o dist",
    "lint": "eslint src --ext ts --max-warnings=0",
    "lint:fix": "eslint src --ext ts --max-warnings=0 --fix",
    "prettier": "prettier --ignore-path \"dist/**/*.js\" --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --ignore-path \"dist/**/*.js\" --check \"src/**/*.ts\" --write",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larahfelipe/saturn.git"
  },
  "author": "larahfelipe",
  "dependencies": {
    "@discordjs/rest": "^1.2.0",
    "@discordjs/voice": "^0.16.0",
    "@prisma/client": "^5.4.2",
    "axios": "^1.5.1",
    "discord.js": "^14.5.0",
    "dotenv": "^16.3.1",
    "ffmpeg": "^0.0.4",
    "glob": "^8.0.3",
    "libsodium-wrappers": "^0.7.13",
    "node-vibrant": "^3.2.1-alpha.1",
    "prisma": "^5.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "0.0.9",
    "typescript": "^5.2.2",
    "yt-search": "^2.10.4",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/yt-search": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "ts-node-dev": "^2.0.0"
  }
}
