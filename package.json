    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.4.1",
    "ts-node-dev": "^1.1.8"
  },
  "engines": {
    "node": "14.16.1"
  }
