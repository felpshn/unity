  "bugs": {
    "url": "https://github.com/larafe1/saturn-bot/issues"
  },
  "homepage": "https://github.com/larafe1/saturn-bot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.6.0",
    "axios": "^0.23.0",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "ffmpeg": "0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "mongoose": "^6.0.12",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "0.0.9",
    "typescript": "^4.4.4",
    "yt-search": "^2.10.2",
    "ytdl-core": "^4.9.1"
