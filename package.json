{
  "name": "saturn-bot",
  "version": "0.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node -r ts-node/register -r tsconfig-paths/register dist/index.js",
    "dev": "set NODE_ENV=development&&tsnd --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc --project tsconfig.json&&tscpaths -p tsconfig.json -s src -o dist",
    "lint": "eslint src --ext ts --max-warnings=0",
    "lint:fix": "eslint src --ext ts --max-warnings=0 --fix",
    "prettier": "prettier --ignore-path \"dist/**/*.js\" --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --ignore-path \"dist/**/*.js\" --check \"src/**/*.ts\" --write",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larafe1/saturn-bot.git"
  },
  "author": "larafe1",
  "license": "GNU GPL v3.0",
  "bugs": {
    "url": "https://github.com/larafe1/saturn-bot/issues"
  },
  "homepage": "https://github.com/larafe1/saturn-bot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "axios": "^0.24.0",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "ffmpeg": "0.0.4",
    "mongoose": "^6.1.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "0.0.9",
    "typescript": "^4.5.4",
    "yt-search": "^2.10.2",
    "ytdl-core": "^4.9.2"
  },
  "devDependencies": {
    "@types/yt-search": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import-helpers": "^1.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.5.1",
    "ts-node-dev": "^1.1.8"
  },
  "engines": {
    "node": "14.16.1"
  }
}
